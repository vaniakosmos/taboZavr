{
  "manifest_version": 2,
  "name": "TaboZavr",
  "description": "This extension helps customize New Tab page",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "img/icon180.png"
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "permissions": [
    "storage",
    "bookmarks",
    "management",
    "tabs",
    "topSites",
    "sessions",
    "chrome://favicon/",
    "unlimitedStorage"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://suggestqueries.google.com; object-src 'self'; "
}
